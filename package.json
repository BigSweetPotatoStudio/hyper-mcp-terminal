{
  "name": "dadigua-web-terminal-cli",
  "version": "1.0.3",
  "description": "Hello World!",
  "main": "main.js",
  "author": "Jane Doe",
  "license": "MIT",
  "bin": {
    "web-terminal": "cli.js"
  },
  "scripts": {
    "dev": "nodemon --watch src -e ts,tsx --exec ts-node src/server.ts",
    "build": "npx tsc -p tsconfig.node.json",
    "build:web": "npx webpack",
    "dev:web": "npx webpack -w",
    "start": "pm2 start lib/server.js --name terminal",
    "restart": "pm2 stop terminal && npm start",
    "prepublish": "npm run build:web && npm run build"
  },
  "devDependencies": {
    "css-loader": "^6.7.1",
    "typescript": "^4.7.3",
    "@ant-design/icons": "^4.7.0",
    "moment": "^2.29.3",
    "querystring-es3": "^0.2.1",
    "react": "^18.1.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1",
    "react-dom": "^18.1.0",
    "@types/node": "^17.0.40",
    "antd": "4.0"
  },
  "dependencies": {
    "commander": "^9.3.0",
    "koa": "^2.13.4",
    "koa-static": "^5.0.0",
    "node-pty": "^0.10.1",
    "nodemon": "^2.0.16",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0"
  }
}