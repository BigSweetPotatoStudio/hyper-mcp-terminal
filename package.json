{
  "name": "hyper-mcp-terminal",
  "version": "1.2.0",
  "description": "Hello World!",
  "main": "dist/electron/main.js",
  "type": "module",
  "author": "Jane Doe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigSweetPotatoStudio/hyper-mcp-terminal"
  },
  "scripts": {
    "build": "npx tsc",
    "build:web": "npx webpack --mode production",
    "dev:web": "npx webpack -w",
    "electron": "electron .",
    "electron:build": "npm run build && npm run build:web && electron-builder",
    "electron:pack": "electron-builder --dir",
    "rebuild": "electron-rebuild"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/koa-static": "^4.0.4",
    "@types/node": "^22.13.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "antd": "^5.26.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.1",
    "electron": "31.0.2",
    "electron-builder": "24.13.3",
    "electron-rebuild": "^3.2.9",
    "html-webpack-plugin": "^5.6.3",
    "moment": "^2.29.3",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "querystring-es3": "^0.2.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "wait-on": "^7.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@types/express": "^4.17.21",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "commander": "^9.3.0",
    "cross-spawn": "^7.0.6",
    "dayjs": "^1.11.13",
    "express": "^4.18.2",
    "log4js": "^6.9.1",
    "node-pty": "^1.0.0",
    "nodemon": "^2.0.16",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "strip-ansi": "^7.1.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "build": {
    "appId": "com.bigsweet.hyper-mcp-terminal",
    "productName": "Hyper MCP Terminal",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "build/**/*",
      "dist/electron/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
